# Visual Studio 2015 user specific files
.vs/

# Compiled Object files
*.slo
*.lo
*.o
*.obj

# Precompiled Headers
*.gch
*.pch

# Compiled Dynamic libraries
*.so
*.dylib
*.dll

# Fortran module files
*.mod

# Compiled Static libraries
*.lai
*.la
*.a
*.lib

# Executables
*.exe
*.out
*.app
*.ipa

# These project files can be generated by the engine
*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
BallRunner_IN/SourceArt/**/*.png
BallRunner_IN/SourceArt/**/*.tga

# Binary Files
BallRunner_IN/Binaries/*
BallRunner_IN/Plugins/*/Binaries/*

# Builds
BallRunner_IN/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!Build/*/
BallRunner_IN/Build/*/**
!Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!Build/**/*.ico

# Built data for maps
*_BuiltData.uasset

# Configuration files generated by the Editor
BallRunner_IN/Saved/*

# Compiled source files for the engine to use
BallRunner_IN/Intermediate/*
BallRunner_IN/Plugins/*/Intermediate/*

# Cache files for the editor to use
BallRunner_IN/DerivedDataCache/*
